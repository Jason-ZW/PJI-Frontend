.cu-list.menu>.cu-item .content {
    line-height: 1.1em;
    flex: 1;
    font-size: 30rpx;
}

.margin-tab-bottom {
    margin-bottom: 120rpx;
}

.leader-text {
    width: 100rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cu-btn.lg {
    padding: 0 30rpx;
    font-size: 32rpx;
    height: 120rpx;
}

.case-info{
    text-align: center;
    padding: 0!important;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    font-size: 20rpx;
}

.case-info-detail {
    margin-top: 15rpx;
    margin-bottom: 15rpx;
    margin-left: 15rpx;
}

.margin-btn {
    margin: 0rpx 30rpx 15rpx 30rpx;
}

.content-item {
    background-color: #ffffff;
    padding: 15rpx 20rpx;
    font-size: 20rpx;
}

.border-blue {
    border-bottom: 2px solid #00A8F9;
    padding-bottom: 10rpx;
}

.padding-special {
    padding: 15rpx;
}