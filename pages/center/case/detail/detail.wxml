<view class="cu-custom" style="height:{{CustomBar}}px;">
    <view class="cu-bar fixed bg-gradual-green" style="height:{{CustomBar}}px;padding-top:{{StatusBar}}px;">
        <navigator class='action border-custom' open-type="navigateBack" delta="1" hover-class="none" style='width:{{Custom.width}}px;height:{{Custom.height}}px;margin-left:calc(750rpx - {{Custom.right}}px)'>
            <text class='icon-back'></text>
            <text class='icon-homefill'></text>
        </navigator>
        <view class='content' style='top:{{StatusBar}}px;'>病历录入</view>
    </view>
</view>
<view class="cu-bar bg-white fixed" style="top:{{CustomBar}}px;">
    <view class="flex">
        <view class='avatar-group'>
            <view class="cu-avatar-group">
                <view class='cu-tag radius' style="margin-right: 40rpx;">填</view>
                <view class="cu-avatar round" wx:for="{{1}}" wx:key style="background-image:url(https://image.weilanwl.com/img/square-{{index+1}}.jpg);"></view>
            </view>
        </view>
    </view>
    <view class="flex">
        <view class='avatar-group'>
            <view class="cu-avatar-group">
                <view class='cu-tag radius' style="margin-right: 40rpx;">修</view>
                <view class="cu-avatar round" wx:for="{{4}}" wx:key style="background-image:url(https://image.weilanwl.com/img/square-{{index+1}}.jpg);"></view>
            </view>
        </view>
    </view>
    <view class="flex">
        <view class='avatar-group'>
            <view class="cu-avatar-group">
                <view class='cu-tag radius' style="margin-right: 40rpx;">审</view>
                <view class="cu-avatar round" wx:for="{{1}}" wx:key style="background-image:url(https://image.weilanwl.com/img/square-{{index+1}}.jpg);"></view>
            </view>
        </view>
    </view>
</view>
<scroll-view scroll-y class="indexes margin-bottom" scroll-into-view="indexes-{{listCurID}}" style='padding-top: 100rpx;height:calc(100vh - {{CustomBar}}px)' scroll-with-animation="true" enable-back-to-top="true">
    <view class="VerticalBox">
        <scroll-view class="VerticalNav nav" scroll-y scroll-with-animation scroll-top="{{VerticalNavTop}}" style="height:calc(100vh - 330rpx)">
            <view class="cu-item {{index==TabCur?'text-green cur':''}}" style="font-size: 22rpx" wx:for="{{TabTitle}}" wx:key bindtap='tabSelect' data-id="{{index}}">
                {{item}}
            </view>
        </scroll-view>
        <!-- 基本信息 -->
        <scroll-view class="VerticalMain" scroll-y style="height:calc(100vh - 330rpx)" wx:if="{{ShowBasic}}">
            <view class="padding-left-special">
                <view class='cu-bar solid-bottom bg-white'>
                    <view class='action'>
                        <text class='icon-title text-green'></text> 基本信息</view>
                </view>
                <view class="cu-list menu menu-avatar">
                    <view class="cu-item-special">
                        <view class="cu-form-group disabled">
                            <view class="title">所属中心</view>
                            <input placeholder="暂无所属中心" class='radius' name='input' value="{{centerName}}" disabled></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>姓名</view>
                            <input placeholder="请输入姓名" class='radius' bindinput="onNameInput" value=""></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>ID</view>
                            <input placeholder="请输入病历号" class='radius' bindinput="onCaseNOInput" value=""></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>建档日期</view>
                            <picker mode="date" value="{{createDate}}" start="2015-01-01" end="2025-01-01" bindchange="onCreateDateChange">
                                <view class="picker">
                                    {{createDate}}
                                </view>
                            </picker>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>性别</view>
                            <picker bindchange="onSexChange" value="{{sex}}" range="{{sexPicker}}">
                                <view class="picker">
                                    {{sexPicker[sex]}}
                                </view>
                            </picker>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>年龄</view>
                            <input placeholder="请输入年龄" class='radius' bindinput="onAgeInput" value=""></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>身高(m)</view>
                            <input placeholder="请注意单位换算" class='radius' bindinput="onHeightInput" value=""></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>体重(Kg)</view>
                            <input placeholder="请注意单位换算" class='radius' bindinput="onWeightInput" value=""></input>
                        </view>
                        <view class="cu-form-group disabled">
                            <view class='title'>BMI(Kg/m2)</view>
                            <input class='radius' name='input' value="{{bmi}}" placeholder="自动生成" disabled></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>主诊医师</view>
                            <input placeholder="请输入主诊医师姓名" class='radius' bindinput="onChiefDocInput" value=""></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>联系电话1</view>
                            <input placeholder="请输入联系方式" class='radius' bindinput="onTel1Input" value=""></input>
                        </view>
                        <view class="cu-form-group {{tel2Disabled?'disabled':''}}">
                            <view class='title'>联系电话2</view>
                            <input placeholder="请输入联系方式" class='radius' bindinput="onTel2Input" value='' disabled='{{tel2Disabled}}'></input>
                            <switch class='green sm' bindchange="onTel2SwitchChange"></switch>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>部位</view>
                            <picker bindchange="onPartChange" value="{{part}}" range="{{partPicker}}">
                                <view class="picker">
                                    {{partPicker[part]}}
                                </view>
                            </picker>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>类型</view>
                            <picker bindchange="onTypeChange" value="{{type}}" range="{{typePicker}}">
                                <view class="picker">
                                    {{typePicker[type]}}
                                </view>
                            </picker>
                        </view>
                        <view class="cu-form-group {{operationDisabled?'disabled':''}}">
                            <view class='title'>末次手术至今时长</view>
                            <picker mode="multiSelector" disabled='{{operationDisabled}}' bindchange="operationDateChange" bindcolumnchange="operationDateColumnChange" value="{{operationDateMultiIndex}}" range="{{operationDateMultiArray}}">
                                <view class="picker">
                                    {{operationDateMultiArray[2][operationDateMultiIndex[2]]?operationDateMultiArray[2][operationDateMultiIndex[2]]:'请选择'}}
                                </view>
                            </picker>
                            <switch class='green sm' style="margin-left: 15rpx;" bindchange="onOperationcSwitchChange"></switch>
                        </view>
                        <view class="cu-form-group {{symptomDisabled?'disabled':''}}">
                            <view class='title'>症状出现时长</view>
                            <picker mode="multiSelector" disabled='{{symptomDisabled}}' bindchange="symptomDateChange" bindcolumnchange="symptomDateColumnChange" value="{{symptomDateMultiIndex}}" range="{{symptomDateMultiArray}}">
                                <view class="picker">
                                    {{symptomDateMultiArray[2][symptomDateMultiIndex[2]]?symptomDateMultiArray[2][symptomDateMultiIndex[2]]:'请选择'}}
                                </view>
                            </picker>
                            <switch class='green sm' style="margin-left: 15rpx;" bindchange="onSymptomSwitchChange"></switch>
                        </view>
                        <view class="cu-form-group disabled">
                            <view class='title'>性质</view>
                            <input class='radius' name='input' value="" placeholder="自动生成" disabled></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>是否合并风湿免疫性疾病</view>
                            <picker bindchange="onRisChange" value="{{ris}}" range="{{risPicker}}">
                                <view class="picker">
                                    {{risPicker[ris]}}
                                </view>
                            </picker>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>其他伴随疾病</view>
                            <input placeholder='请输入文字, 没有请填"无"' class='radius' bindinput="onQtbsjbInput" value=""></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>近期(2周)应用抗生素</view>
                            <picker bindchange="onAntibioticChange" value="{{antibiotic}}" range="{{antibioticPicker}}">
                                <view class="picker">
                                    {{antibioticPicker[antibiotic]}}
                                </view>
                            </picker>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>简要病史</view>
                            <input placeholder='请输入文字, 没有请填"无"' class='radius' bindinput="onJybsInput" value=""></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>初步诊断</view>
                            <input placeholder='请输入文字, 没有请填"无"' class='radius' bindinput="onCbzdInput" value=""></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>特殊事项备注</view>
                            <input placeholder='请输入文字, 没有请填"无"' class='radius' bindinput="onTsssbzInput" value=""></input>
                        </view>
                    </view>
                </view>
            </view>
        </scroll-view>
        <!-- 诊断性穿刺 -->
        <scroll-view class="VerticalMain" scroll-y style="height:calc(100vh - 330rpx)" wx:if="{{ShowDiagnose}}">
            <view class="padding-left-special">
                <view class='cu-bar solid-bottom bg-white'>
                    <view class='action'>
                        <text class='icon-title text-green'></text> 穿刺后信息</view>
                </view>
                <!-- item -->
                <view class="cu-list menu menu-avatar">
                    <view class="cu-item-special">
                        <!-- 穿刺日期 -->
                        <view class="cu-form-group  {{ccDateDisabled?'disabled':''}}">
                            <view class='title'>穿刺日期</view>
                            <picker mode="date" value="{{chuangciDate}}" start="2015-01-01" end="2025-01-01" bindchange="onChuanciDateChange" disabled='{{ccDateDisabled}}'>
                                <view class="picker">
                                    {{chuangciDate}}
                                </view>
                            </picker>
                            <switch class='green sm' bindchange="onCCDateSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 穿刺描述 -->
                        <view class="cu-form-group {{ccDescribeDisabeld?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>穿刺描述</view>
                                <input placeholder="请简述穿刺时一般情况" class='radius' style='width: 350rpx;text-align:left;' type='number' bindinput="onCCDescribeInput" disabled='{{ccDescribeDisabeld}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onCCDescribeSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 抽出关节液总量 -->
                        <view class="cu-form-group {{ccgjyDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>抽出关节液总量(ml)</view>
                                <input placeholder="请输入数字，不适用请点击右侧" class='radius' style='width: 350rpx;text-align:left;' type='number' bindinput="onCcgjyInput" disabled='{{ccgjyDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onCcgjySwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 抽出灌洗液总量 -->
                        <view class="cu-form-group {{ccgxyDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>抽出灌洗液总量(ml)</view>
                                <input placeholder="请输入数字，不适用请点击右侧" class='radius' style='width: 350rpx;text-align:left;' type='number' bindinput="onCcgxyInput" disabled='{{ccgxyDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onCcgxySwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- LE试纸（原液） -->
                        <view class="cu-form-group {{leDisabled?'disabled':''}}">
                            <view class='title'>LE试纸(原液)</view>
                            <picker bindchange="onLeChange" value="{{leIndex}}" range="{{lePicker}}" disabled='{{leDisabled}}'>
                                <view class="picker">
                                    {{lePicker[leIndex]}}
                                </view>
                            </picker>
                            <button class='cu-btn round sm' style='margin-left: 15rpx;' disabled='{{leDisabled}}' bindtap="showModal" data-target="LEDrawerModalR">上传图片</button>
                            <switch class='green sm' bindchange="onLeSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- LE试纸（离心后） -->
                        <view class="cu-form-group {{leAfterDisabled?'disabled':''}}">
                            <view class='title'>LE试纸(离心后)</view>
                            <picker bindchange="onLeAfterChange" value="{{leAfterIndex}}" range="{{leAfterPicker}}" disabled='{{leAfterDisabled}}'>
                                <view class="picker">
                                    {{leAfterPicker[leAfterIndex]}}
                                </view>
                            </picker>
                            <button class='cu-btn round sm' style='margin-left: 15rpx;' disabled='{{leAfterDisabled}}' bindtap="showModal" data-target="LEAfterDrawerModalR">上传图片</button>
                            <switch class='green sm' bindchange="onLeAfterSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 关节液白细胞计数（cells/μL） -->
                        <view class="cu-form-group {{gjybxbDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>关节液白细胞计数(cells/μL)</view>
                                <input placeholder="请输入数字，请注意单位换算" class='radius' style='width: 350rpx;text-align:left;' type='number' bindinput="onGjybxbInput" disabled='{{gjybxbDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onGjybxbSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 关节液中性粒细胞百分比（0.01） -->
                        <view class="cu-form-group {{gjyzxDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>关节液中性粒细胞百分比(0.01)</view>
                                <input placeholder="请注意格式，保留小数点2位" style=' width: 350rpx;text-align:left;' class='radius' type='digit' bindinput="onGjyzxInput" disabled='{{gjyzxDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onGjyzxSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 本次培养送检类型 -->
                        <view class="cu-form-group {{bcpysjDisabled?'disabled':''}}">
                            <view class='title'>本次培养送检类型</view>
                            <picker bindchange="onBcpysjChange" value="{{bcpysjIndex}}" range="{{bcpysjPicker}}" disabled='{{bcpysjDisabled}}'>
                                <view class="picker">
                                    {{bcpysjPicker[bcpysjIndex]}}
                                </view>
                            </picker>
                            <switch class='green sm' bindchange="onBcpysjSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 打入各培养瓶量（ml） -->
                        <view class="cu-form-group {{drgpypDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>打入各培养瓶量(ml)</view>
                                <input style=' width: 350rpx;text-algin:left;' class='radius' type='digit' style='text-align:left;' bindinput="onDrgpypjChange" disabled='{{drgpypDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onDrgpypSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 本次需氧+真菌培养结果 -->
                        <view class="cu-form-group {{bcxyResultDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>本次需氧+真菌培养结果</view>
                                <input placeholder="请输入文字，报告日期+微生物名称" style='width: 380rpx;text-align:left;' class='radius' type='digit' bindinput="onBcxyChange" disabled='{{bcxyResultDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onBcxySwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 本次需氧+真菌培养时长（天） -->
                        <view class="cu-form-group {{bcxyLastDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>本次需氧+真菌培养时长(天)</view>
                                <input placeholder="请输入数字" style='width: 350rpx;text-align:left;' class='radius' type='digit' bindinput="onBcxyLastChange" disabled='{{bcxyLastDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onBcxyLastSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 本次厌氧培养结果 -->
                        <view class="cu-form-group {{bcyyResultDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>本次厌氧培养结果</view>
                                <input placeholder="请输入文字，报告日期+微生物名称" style='width: 350rpx;text-align:left;' class='radius' type='digit' bindinput="onBcyyChange" disabled='{{bcyyResultDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onBcyySwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 本次厌氧培养时长（天） -->
                        <view class="cu-form-group {{bcyyLastDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>本次厌氧培养时长(天)</view>
                                <input placeholder="请输入数字" style='width: 350rpx;text-align:left;' class='radius' type='digit' bindinput="onBcyyLastChange" disabled='{{bcyyLastDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onBcyyLastSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 关节液/冲洗液mNGS结果 -->
                        <view class="cu-form-group {{mNGSResultDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>关节液/冲洗液mNGS结果</view>
                                <input placeholder="请输入文字，报告日期+微生物名称" style='width: 350rpx;text-align:left;' class='radius' type='digit' bindinput="onMNGsChange" disabled='{{mNGSResultDisabled}}'></input>
                            </view>
                            <switch class='green sm' bindchange="onMNGSSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                    </view>
                </view>
            </view>
        </scroll-view>
        <!-- 入院后信息 -->
        <scroll-view class="VerticalMain" scroll-y style="height:calc(100vh - 330rpx)" wx:if="{{ShowAdmission}}">
            <view class="padding-left-special">
                <view class='cu-bar solid-bottom bg-white'>
                    <view class='action'>
                        <text class='icon-title text-green'></text> 入院后信息</view>
                </view>
                <view class="cu-list menu menu-avatar">
                    <view class="cu-item-special">
                        <!-- 窦道 -->
                        <view class="cu-form-group">
                            <view class='title'>窦道</view>
                            <picker bindchange="onDoudaoChange" value="{{doudaoIndex}}" range="{{doudaoPicker}}">
                                <view class="picker">
                                    {{doudaoPicker[doudaoIndex]}}
                                </view>
                            </picker>
                        </view>
                        <!-- 术前ESR (mm/h) -->
                        <view class="cu-form-group">
                            <view class='title'>术前ESR(mm/h)</view>
                            <input placeholder="请输入数字" class='radius' name='input' type="number" value="" bindinput="onSqesrChange"></input>
                        </view>
                        <!-- 术前CRP (mg/dl)-->
                        <view class="cu-form-group">
                            <view class='title'>术前CRP(mg/dl)</view>
                            <input placeholder="请输入数字，注意单位换算" class='radius' name='input' type="number" value="" bindinput="onSqcrpChange"></input>
                        </view>
                        <!-- 标准化的CRP (mg/L) -->
                        <view class="cu-form-group disabled">
                            <view class='title'>标准化的CRP(mg/L)</view>
                            <input class='radius' placeholder='默认生成，术前CRP乘10' name='input' type="number" value="{{bzhcrp}}" disabled='true'></input>
                        </view>
                        <!-- IL-6 (pg/ml) -->
                        <view class="cu-form-group {{mNGSResultDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column;'>
                                <view class='title' style='text-align:left'>IL-6(pg/ml)</view>
                                <input placeholder="请输入数字，未测请点右侧" class='radius' disabled='{{mNGSResultDisabled}}' type="number" style='width: 350rpx;text-align:left;' bindinput="onSqesrChange"></input>
                            </view>
                            <switch class='green sm' bindchange="onMNGSSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 纤维蛋白原 (μg/ml) -->
                        <view class="cu-form-group {{xwdbyDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column;'>
                                <view class='title' style='text-align:left'>纤维蛋白原(μg/ml)</view>
                                <input placeholder="请输入数字，未测请点右侧" class='radius' disabled='{{xwdbyDisabled}}' type="number" style='width: 350rpx;text-align:left;' bindinput="onXwdbyChange"></input>
                            </view>
                            <switch class='green sm' bindchange="onXwdbySwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- D-dimer (μg/ml) -->
                        <view class="cu-form-group {{ddimerDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column;'>
                                <view class='title' style='text-align:left'>D-dimer(μg/ml)</view>
                                <input placeholder="请输入数字，未测请点右侧" class='radius' disabled='{{ddimerDisabled}}' type="number" style='width: 350rpx;text-align:left;' bindinput="onDdimerChange"></input>
                            </view>
                            <switch class='green sm' bindchange="onDdimerSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 手术日期 -->
                        <view class="cu-form-group  {{ssDateDisabled?'disabled':''}}">
                            <view class='title'>手术日期</view>
                            <picker mode="date" value="{{shoushuDate}}" start="2015-01-01" end="2025-01-01" bindchange="onShoushuDateChange" disabled='{{ssDateDisabled}}'>
                                <view class="picker">
                                    {{chuangciDate}}
                                </view>
                            </picker>
                            <switch class='green sm' bindchange="onShoushuSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 术中见脓 -->
                        <view class="cu-form-group {{szjnDisabled?'disabled':''}}">
                            <view class='title'>术中见脓</view>
                            <picker bindchange="onSzjnChange" value="{{szjnIndex}}" range="{{szjnPicker}}" disabled='{{szjnDisabled}}'>
                                <view class="picker">
                                    {{szjnPicker[szjnIndex]}}
                                </view>
                            </picker>
                            <switch class='green sm' bindchange="onSzjnSwitchChange" style='margin-left: 15rpx;'></switch>
                        </view>
                        <!-- 病理 -->
