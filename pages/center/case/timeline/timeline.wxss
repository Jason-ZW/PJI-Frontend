@import "../../../../font-awesome.wxss";

.title-container {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    width: 100%;
    font-size: 16px;
    padding: 10rpx;
}

.title-container> view {
    margin-left: 20rpx;
}

.btn-container {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    width: 100%;
    font-size: 16px;
    padding: 20rpx;
}

.btn-container> button {
    width: 180rpx;
}

.cu-timeline .cu-time {
    width: 160rpx;
    font-size: 30rpx;
}

.timeline-item-container {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.timeline-item-content {
    display: flex;
    flex-direction: column;
}
