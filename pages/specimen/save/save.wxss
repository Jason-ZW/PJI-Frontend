@import "../../../css/font-awesome.wxss";
@import "../../../css/common.wxss";
@import "../../../css/weui.wxss";
@import "../../../miniprogram_npm/wux-weapp/styles/index.wxss";
page {
  position: absolute;
  top: 0;
  left: 0;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: #f6f8fa;
}
.page__bd {
    width: 100%;
}
.box-info {
    width: 100%;
    height: 40rpx;
    border-bottom: 1px solid #ddd;
    padding: 30rpx 0rpx;
    background-color: #fff;
}
.box-title {
    padding-left: 30rpx;
    font-size: 16px;
}
.box-description {
    padding-left: 30rpx;
    color: #586069;
    font-size: 12px;
}
.select-view {
    width: 100%;
    height: 700rpx;
    margin-top: 10rpx;
}
.top-ruler {
    width: 100%;
    height: 20px;
}
.top-ruler-view {
    background-color: #e7e7e7;
    border-radius: 10px;
    margin-left: 15rpx;
    margin-right: 15rpx;
}
.ruler-text-top {
    font-size: 16px;
    color: #444;
    margin-top: 10px;
    margin-left: 15rpx;
}
.left-ruler-view {
    margin-top: 15rpx;
    margin-left: 15rpx;
    margin-right: 15rpx;
}